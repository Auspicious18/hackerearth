#include<stdio.h>
#include<string.h>
int main()
{
    int L,R,U,D,x=0,y=0,i;
    char a[1000];
    
    gets(a);
    for(i=0;i<strlen(a);i++)
    {
    //    /*
		if(a[i]=='L')
        	x=x-1;
        else if(a[i]=='R')
        	x=x+1;
        else if(a[i]=='U')
        	y=y+1;
        else if(a[i]=='D')
        	y=y-1;
		//*/
		//printf("%s",a);
    }
    printf("%d %d",x,y);
}
